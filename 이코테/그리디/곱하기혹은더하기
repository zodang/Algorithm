#include <bits/stdc++.h>
using namespace std;

string S;

int main() {
    
    cin >> S;
    
    // 
    int result = S[0] -'0';
    
    for (int i = 1; i < S.size(); i++)
    {
        int currentNum = S[i] -'0';

        if (result <= 1 || currentNum <= 1) result += currentNum;
        else result *= currentNum;
    }
    
    cout << result;
}
